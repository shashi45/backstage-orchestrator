import uvicorn
from a2a.server import A2AStarterApplication
from orchestrator_agent.agent_card import AGENT_CARD
from orchestrator_agent.executor import OrchestratorExecutor

app = A2AStarterApplication(agent_card=AGENT_CARD, executor=OrchestratorExecutor())

if __name__ == "__main__":
    uvicorn.run(app, host="127.0.0.1", port=8000)
